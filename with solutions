# Write a Python program to create a histogram from a given list of integers


def histogram():
    my_list = []
    my_string = input('Enter a string : ')

    # inserting elements in my_list
    my_list.insert(0, 2)
    my_list.insert(1, 3)
    my_list.insert(2, 6)
    my_list.insert(3, 5)

    # product of my_list[index] and my input string
    value_1 = my_list[0]*my_string
    value_2 = my_list[1]*my_string
    value_3 = my_list[2]*my_string
    value_4 = my_list[3]*my_string

    print(value_1)
    print(value_2)
    print(value_3)
    print(value_4)


histogram()
